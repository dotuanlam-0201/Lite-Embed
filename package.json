{
  "name": "lite-embed",
  "version": "1.0.1",
  "description": "High performance lazy-load embeds for YouTube and Vimeo",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "npm run build  && vitest"
  },
  "keywords": [],
  "author": "DO TUAN LAM",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.0.0",
    "@testing-library/dom": "^10.4.1",
    "@types/jsdom": "^28.0.0",
    "jsdom": "24",
    "rollup": "^3.0.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.18"
  }
}
